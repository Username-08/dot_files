! xmodmap for the Colemak Mod-DH layout (US, ANSI keyboard, Z angle mod, Wide layout).
! http://colemakmods.github.io/mod-dh/.
! Public domain.

keycode  49  =  grave         asciitilde   
keycode  10  =  1             exclam       
keycode  11  =  2             at           
keycode  12  =  3             numbersign   
keycode  13  =  4             dollar       
keycode  14  =  5             percent      
keycode  15  =  equal         plus         
keycode  16  =  6             asciicircum  
keycode  17  =  7             ampersand    
keycode  18  =  8             asterisk     
keycode  19  =  9             parenleft    
keycode  20  =  0             parenright   
keycode  21  =  minus         underscore   

keycode  24  =  q             Q             greater
keycode  25  =  w             W             dollar
keycode  26  =  f             F             colon
keycode  27  =  p             P             semicolon
keycode  28  =  b             B             exclam
keycode  29  =  bracketleft   braceleft    
keycode  30  =  j             J             numbersign
keycode  31  =  l             L             percent
keycode  32  =  u             U             asciicircum
keycode  33  =  y             Y             ampersand
keycode  34  =  semicolon     colon        
!keycode  35  =  apostrophe    quotedbl    
keycode  51  =  backslash     bar          

keycode  38  =  a             A             less
keycode  39  =  r             R             bracketleft
keycode  40  =  s             S             braceleft
keycode  41  =  t             T             parenleft
keycode  42  =  g             G             
keycode  43  =  bracketright  braceright   
keycode  44  =  h             H             asterisk
keycode  45  =  n             N             plus
keycode  46  =  e             E             equal
keycode  47  =  i             I             minus
keycode  48  =  o             O             underscore

keycode  94  =  z             Z             
keycode  52  =  x             X             bracketright
keycode  53  =  c             C             braceright
keycode  54  =  d             D             parenright
keycode  55  =  v             V             
keycode  56  =  z             Z            
keycode  57  =  slash         question     
keycode  58  =  k             K            
keycode  59  =  m             M            
keycode  60  =  comma         less         
keycode  61  =  period        greater      

keycode  65  =  space         space         space

keycode 108 =  Mode_switch Mode_switch

! Remove these 2 lines if you want caps lock unmodified
keycode 66 =  Escape Escape Escape Escape
clear Lock

clear Shift
!clear Control
!clear Mod1
!clear Mod2
!clear Mod3
!clear Mod4
!clear Mod5

keycode  50 = Shift_L
keycode 35 = Shift_R
keycode  62  =  apostrophe    quotedbl     otilde                Otilde
add    Shift   = Shift_L Shift_R
!add    Control = Control_L Control_R
!add    Mod1    = Alt_L Alt_R
!add    Mod2    = Num_Lock
!add    Mod4    = Meta_L Meta_R
!add    Mod5    = Scroll_Lock 

!keycode   9 = Caps_Lock NoSymbol Caps_Lock
